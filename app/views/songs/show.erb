<h1> My Song </h1>
<h1>My edit</h1>
<%= form_for @song do |f| %>
    <%= f.label "My song name" %>
    <%= f.text_field :name %>
    <%= f.label "My Artist" %>
    <%= link_to 'My Artist', artist_path(@artist) %>
    <%= f.text_field :artist_id %>
    <%= f.label "My Genre" %>
    <%= link_to 'My Genre', genre_path(@genre) %>
    <%= f.text_field :genre_id %>
    <%= f.submit %>
<% end %>